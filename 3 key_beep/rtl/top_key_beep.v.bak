module top_key_beep(
	input 		   sys_clk,
	input          rst_n,
	
	input 			key,
	output reg 		beep
);

wire key_value;
wire key_flag;

key_debounce u_key_debounce(
	.sys_clk			(sys_clk),
	.rst_n			(rst_n),
			
	.key				(key),
	.key_value		(key_value),
	.key_flag		(key_flag)	
);

beep_control u_beep_control(
	.sys_clk			(sys_clk),
	.rst_n			(rst_n),
	
	.key_flag		(key_flag),
	.key_value		(key_value),
	.beep				(beep)
);

endmodule
